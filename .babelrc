{
	"plugins": [
		["transform-es2015-destructuring", { "loose": false }],
		["transform-es2015-modules-commonjs", { "loose": false }],
		"transform-es2015-parameters",
		"transform-es2015-spread",
	],
	"env": {
		"development": {
			"presets": [
				"stage-0-without-async",
			],
		},
		"node4": {
			"presets": [
				"stage-0",
			],
		},
	}
}
